<div class="gallery">
  <% @parks.each do |park| %>
    <div class="card">
      <div class="image">
        <img src="<%= park.image_url %>">
      </div>
      <div class="name">
        <%= link_to park.name, park %>
      </div>
    </div>
  <% end %>
</div>

<%= link_to "Add Park", new_park_path %>
