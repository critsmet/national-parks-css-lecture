<%= form_for @park do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :size %>
  <%= f.number_field :size %>

  <%= f.label :ticket_cost %>
  <%= f.number_field :ticket_cost %>

  <%= f.label :image_url, "Image Url" %>
  <%= f.text_field :image_url %>

  <%= collection_select(:park, :state_id, State.all, :id, :name, prompt: true) %>

  <%= f.submit %>

<% end %>
